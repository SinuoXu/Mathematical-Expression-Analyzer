{
  "task1_expression_analysis": {
    "description": "Task 1: 表达式分析 - 分词和AST生成",
    "test_cases": [
      {
        "category": "基础表达式",
        "expressions": [
          "x + 1",
          "2x",
          "x^2",
          "x - y",
          "3*x",
          "x/2"
        ]
      },
      {
        "category": "多项式展开",
        "expressions": [
          "(x+1)^2",
          "(x+y)^2",
          "(x-1)^2"
        ]
      },
      {
        "category": "函数调用",
        "expressions": [
          "sin(x)",
          "cos(x+y)",
          "ln(x)",
          "sqrt(x^2)"
        ]
      },
      {
        "category": "复杂表达式",
        "expressions": [
          "x^2 / 2",
          "1 - 1/x",
          "x^3 + 2x^2 - x + 1",
          "(x+1)*(x-1)"
        ]
      },
      {
        "category": "隐式乘法",
        "expressions": [
          "2(x+1)",
          "xy",
          "sin(x)cos(x)",
          "3x^2",
          "x(y+z)"
        ]
      },
      {
        "category": "一元负号",
        "expressions": [
          "-x",
          "-x^2",
          "-(x+1)",
          "-sin(x)",
          "x + (-y)"
        ]
      },
      {
        "category": "嵌套括号",
        "expressions": [
          "((x))",
          "(x+(y+z))",
          "((x+y)*(x-y))"
        ]
      },
      {
        "category": "边界条件 - 单项",
        "expressions": [
          "0",
          "1",
          "x",
          "42",
          "-5"
        ]
      },
      {
        "category": "边界条件 - 运算符优先级",
        "expressions": [
          "x+y*z",
          "x*y+z",
          "x^2+y^2",
          "x+y^2*z",
          "(x+y)^2*z"
        ]
      },
      {
        "category": "边界条件 - 复杂嵌套",
        "expressions": [
          "sin(cos(x))",
          "sin(x^2)",
          "x^(2+1)",
          "(x^2)^3",
          "ln(sin(x))"
        ]
      },
      {
        "category": "边界条件 - 多变量",
        "expressions": [
          "xyz",
          "x+y+z",
          "x*y*z",
          "x+y*z"
        ]
      },
      {
        "category": "边界条件 - 分数和有理式",
        "expressions": [
          "1/x",
          "x/y",
          "(x+1)/(x-1)",
          "1/(x+1)",
          "x/(y+z)"
        ]
      },
      {
        "category": "边界条件 - 幂次",
        "expressions": [
          "x^0",
          "x^1",
          "2^x",
          "x^x",
          "x^(y^z)"
        ]
      }
    ]
  },
  "task2_equivalence_checking": {
    "description": "Task 2: 等价性检查 - 判断两个表达式是否相等",
    "test_cases": [
      {
        "category": "交换律",
        "should_be_equivalent": true,
        "pairs": [
          ["x + 1", "1 + x"],
          ["x * y", "y * x"],
          ["sin(x) + cos(x)", "cos(x) + sin(x)"],
          ["x + y + z", "z + y + x"]
        ]
      },
      {
        "category": "多项式展开",
        "should_be_equivalent": true,
        "pairs": [
          ["(x+1)^2", "x^2 + 2x + 1"],
          ["(x+y)^2", "x^2 + 2xy + y^2"],
          ["(x+1)^3", "x^3 + 3x^2 + 3x + 1"],
          ["(x-1)^2", "x^2 - 2x + 1"],
          ["(x+1)*(x-1)", "x^2 - 1"]
        ]
      },
      {
        "category": "分配律",
        "should_be_equivalent": true,
        "pairs": [
          ["2(x+1)", "2x + 2"],
          ["x(y+z)", "xy + xz"],
          ["(x+y)*z", "xz + yz"],
          ["x(x+1)", "x^2 + x"]
        ]
      },
      {
        "category": "结合律",
        "should_be_equivalent": true,
        "pairs": [
          ["(x+y)+z", "x+(y+z)"],
          ["(x*y)*z", "x*(y*z)"]
        ]
      },
      {
        "category": "有理式化简",
        "should_be_equivalent": true,
        "pairs": [
          ["1-1/x", "(x-1)/x"],
          ["x/x", "1"],
          ["2/x + 3/x", "5/x"],
          ["x/2 + x/2", "x"],
          ["(x+1)/x", "1 + 1/x"]
        ]
      },
      {
        "category": "函数参数",
        "should_be_equivalent": true,
        "pairs": [
          ["sin(x+y)", "sin(y+x)"],
          ["ln(x*y)", "ln(y*x)"],
          ["cos(2x)", "cos(x*2)"],
          ["sin(x+0)", "sin(x)"]
        ]
      },
      {
        "category": "隐式乘法",
        "should_be_equivalent": true,
        "pairs": [
          ["2x", "x*2"],
          ["xy", "x*y"],
          ["2(x+1)", "2*(x+1)"],
          ["3xy", "3*x*y"],
          ["2x^2", "2*(x^2)"]
        ]
      },
      {
        "category": "恒等元",
        "should_be_equivalent": true,
        "pairs": [
          ["x + 0", "x"],
          ["x * 1", "x"],
          ["x - 0", "x"]
        ]
      },
      {
        "category": "零元",
        "should_be_equivalent": true,
        "pairs": [
          ["x * 0", "0"],
          ["0 * x", "0"]
        ]
      },
      {
        "category": "一元负号",
        "should_be_equivalent": true,
        "pairs": [
          ["-x + x", "0"],
          ["-(x+y)", "-x-y"],
          ["-2*x", "-(2*x)"],
          ["(-1)*x", "-x"]
        ]
      },
      {
        "category": "幂次运算",
        "should_be_equivalent": true,
        "pairs": [
          ["x^1", "x"],
          ["x^2*x", "x^3"],
          ["x*x", "x^2"]
        ]
      },
      {
        "category": "因式分解",
        "should_be_equivalent": true,
        "pairs": [
          ["x*y + x*z", "x*(y+z)"],
          ["x^2 + x", "x*(x+1)"],
          ["2x + 2y", "2*(x+y)"],
          ["xy + xz + x", "x*(y+z+1)"]
        ]
      },
      {
        "category": "复合展开",
        "should_be_equivalent": true,
        "pairs": [
          ["(x+1)*(x+2)", "x^2 + 3x + 2"],
          ["(x+y)*(x-y)", "x^2 - y^2"],
          ["(2x+1)^2", "4x^2 + 4x + 1"],
          ["x(x+1)(x-1)", "x^3 - x"]
        ]
      },
      {
        "category": "边界条件 - 幂次特殊情况",
        "should_be_equivalent": true,
        "pairs": [
          ["x^0", "1"],
          ["1^x", "1"],
          ["x^1", "x"]
        ]
      },
      {
        "category": "边界条件 - 分数运算",
        "should_be_equivalent": true,
        "pairs": [
          ["1/(1/x)", "x"],
          ["(x/y)/z", "x/(y*z)"],
          ["x/(y/z)", "x*z/y"]
        ]
      },
      {
        "category": "边界条件 - 负数运算",
        "should_be_equivalent": true,
        "pairs": [
          ["-(-x+y)", "x-y"],
          ["-(x-y)", "-x+y"]
        ]
      },
      {
        "category": "不等价 - 不同系数",
        "should_be_equivalent": false,
        "pairs": [
          ["x+1", "x+2"],
          ["2x", "3x"],
          ["x^2", "2*x^2"]
        ]
      },
      {
        "category": "不等价 - 不同幂次",
        "should_be_equivalent": false,
        "pairs": [
          ["x^2", "x^3"],
          ["x", "x^2"],
          ["x^2 + x", "x^3 + x"]
        ]
      },
      {
        "category": "不等价 - 不同函数",
        "should_be_equivalent": false,
        "pairs": [
          ["sin(x)", "cos(x)"],
          ["ln(x)", "sqrt(x)"]
        ]
      },
      {
        "category": "不等价 - 不同结构",
        "should_be_equivalent": false,
        "pairs": [
          ["x+y", "x*y"],
          ["x^2", "2*x"],
          ["x/y", "y/x"]
        ]
      },
      {
        "category": "不等价 - 函数参数不同",
        "should_be_equivalent": false,
        "pairs": [
          ["sin(x)", "sin(y)"],
          ["sin(x)", "sin(x+1)"]
        ]
      }
    ]
  }
}
